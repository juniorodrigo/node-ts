generator client {
  provider = "prisma-client-js"
}

generator zod {
  provider = "zod-prisma-types"
  output   = "./generated/zod"
}

datasource db {
  provider = "postgresql"
}

model Greetings {
  id   Int    @id @default(autoincrement())
  text String
}
